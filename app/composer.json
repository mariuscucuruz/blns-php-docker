{
    "name": "mariuscucuruz/blns-php-docker",
    "description": "Vanilla project to test the PHP implementation of the Big List of Naughty Strings.",
    "version": "1.0.0",
    "type": "metapackage",
    "license": "MIT",
    "authors": [
        {
            "name": "Marius Cucuruz",
            "email": "marius.cucuruz@gmail.com"
        }
    ],
    "require": {
        "blns/blns": "*",
        "mattsparks/blns-php": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "VanillaPHP\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VanillaPHP\\Tests\\": "test"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "blns/blns",
                "version": "1.0.0",
                "dist": {
                    "url": "https://github.com/minimaxir/big-list-of-naughty-strings/archive/master.zip",
                    "type": "zip"
                },
                "config": {
                    "vendor-dir": "blns"
                }
            }
        }
    ]
}
